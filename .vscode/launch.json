{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug cub (gdb)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/cub",
      "args": ["maps/file.cub"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "externalConsole": false,
      "preLaunchTask": "build"
    }
  ]
}
